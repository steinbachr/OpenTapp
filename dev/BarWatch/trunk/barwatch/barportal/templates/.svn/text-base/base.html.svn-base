<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>    
    {% load staticfiles %}    
    <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
    <link href="{{ STATIC_URL }}css/libraries/bootstrap.min.css" rel="stylesheet" type="text/css"/>
    <link href="{{ STATIC_URL }}css/libraries/bootstrap-responsive.min.css" rel="stylesheet" type="text/css"/>
    {# PLUGINS #}
    <link href="{{ STATIC_URL }}css/plugins/jquery.selectboxit.css" rel="stylesheet" type="text/css"/>
    <link href="{{ STATIC_URL }}css/plugins/timepicker.less" rel="stylesheet/less" type="text/css" />
    {# END PLUGINS #}
    <link rel="stylesheet" href="{{ STATIC_URL }}css/libraries/font-awesome.css" />
    <link rel="shortcut icon" href="{{ STATIC_URL }}images/logo_u.png" />
    {% block css_files %}{% endblock %}
    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}js/libraries/jquery-validation/dist/jquery.validate.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}js/libraries/jquery-validation/dist/additional-methods.js"></script>
    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false"></script>    
    <script type="text/javascript" src="{{ STATIC_URL }}js/libraries/bootstrap.min.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}js/libraries/jquery-ui-1.9.2.custom.min.js" ></script>    
    {# PLUGINS #}    
    <script type="text/javascript" src="{{ STATIC_URL }}js/plugins/jquery.selectboxit.js"></script>
    <script type="text/javascript" src="{{ STATIC_URL }}js/plugins/bootstrap-timepicker.js"></script>
    {# END PLUGINS #}
    <script type="text/javascript" src="{{ STATIC_URL }}js/core.js"></script>
    {% block js_files %}{% endblock %}
    <script type="text/javascript" src="{{ STATIC_URL }}js/libraries/less-1.3.1.min.js" ></script>
    <title>{% block title %}{% endblock %}</title>
</head>
{% block body %}{% endblock %}
</html>
